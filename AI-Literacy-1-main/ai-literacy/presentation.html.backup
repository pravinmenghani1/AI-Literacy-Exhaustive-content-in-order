    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2canvas/1.4.1/html2canvas.min.js"></script>
    <script>
        let currentSlide = 0;
        const slides = document.querySelectorAll('.slide');
        const totalSlides = slides.length;
        let userScore = 0;
        let quizAnswers = {};
        let finalQuizAnswers = {};

        // Progress tracking
        const progressItems = [
            'Introduction', 'AI Hierarchy', 'Intelligence Types', 'Learning Process', 'Pattern Recognition',
            'Loan Example', 'ML vs DL', 'Marshall Analogy', 'Neural Networks', 'Black Box',
            'Discriminative vs Generative', 'GPT-4 Stats', 'AI Types', 'Challenges', 'Key Takeaways',
            'Knowledge Check', 'Hierarchy Activity', 'Reflection', 'Pattern Challenge', 'ML vs DL Quiz',
            'Ethics Challenge', 'Real-world Application', 'Final Quiz 1', 'Final Quiz 2', 'AI Algorithms', 
            'Capstone Intro', 'Capstone Phase 1&2', 'Capstone Phase 3&4', 'Certificate', 'Course Complete'
        ];

        function initializeProgressTracker() {
            const tracker = document.getElementById('progressTracker');
            progressItems.forEach((item, index) => {
                const div = document.createElement('div');
                div.className = 'progress-item';
                div.innerHTML = `
                    <div class="progress-dot ${index === 0 ? 'current' : ''}" id="dot-${index}"></div>
                    <span>${item}</span>
                `;
                tracker.appendChild(div);
            });
        }

        function updateProgressTracker() {
            // Update dots
            document.querySelectorAll('.progress-dot').forEach((dot, index) => {
                dot.classList.remove('current', 'completed');
                if (index < currentSlide) {
                    dot.classList.add('completed');
                } else if (index === currentSlide) {
                    dot.classList.add('current');
                }
            });
        }

        function showSlide(n) {
            slides[currentSlide].classList.remove('active');
            currentSlide = (n + totalSlides) % totalSlides;
            slides[currentSlide].classList.add('active');
            
            // Update progress bar
            const progress = ((currentSlide + 1) / totalSlides) * 100;
            document.getElementById('progressBar').style.width = progress + '%';
            
            // Update slide counter
            document.getElementById('slideCounter').textContent = `${currentSlide + 1} / ${totalSlides}`;
            
            // Update progress tracker
            updateProgressTracker();
        }

        function nextSlide() {
            showSlide(currentSlide + 1);
        }

        function previousSlide() {
            showSlide(currentSlide - 1);
        }

        // Quiz functionality
        function selectQuizOption(element, isCorrect) {
            const options = element.parentNode.querySelectorAll('.quiz-option');
            options.forEach(opt => opt.classList.remove('selected', 'correct', 'incorrect'));
            
            element.classList.add('selected');
            
            setTimeout(() => {
                if (isCorrect) {
                    element.classList.add('correct');
                    userScore += 10;
                    updateScore();
                } else {
                    element.classList.add('incorrect');
                }
                
                const feedback = element.parentNode.nextElementSibling;
                feedback.classList.add('show', isCorrect ? 'correct' : 'incorrect');
            }, 500);
        }

        function selectPatternOption(element, isCorrect) {
            selectQuizOption(element, isCorrect);
        }

        function selectMLOption(element, isCorrect) {
            selectQuizOption(element, isCorrect);
        }

        function selectEthicsOption(element, choice) {
            const options = element.parentNode.querySelectorAll('.quiz-option');
            options.forEach(opt => opt.classList.remove('selected'));
            element.classList.add('selected');
            
            setTimeout(() => {
                const feedback = document.getElementById('ethicsFeedback');
                feedback.classList.add('show');
                if (choice === 'B') {
                    userScore += 15;
                    updateScore();
                    feedback.classList.add('correct');
                } else {
                    feedback.classList.add('incorrect');
                }
            }, 500);
        }

        function selectFinalQuiz(element, questionNum, isCorrect) {
            const options = element.parentNode.querySelectorAll('.quiz-option');
            options.forEach(opt => opt.classList.remove('selected'));
            element.classList.add('selected');
            
            finalQuizAnswers[questionNum] = isCorrect;
        }

        function showFinalResults(part) {
            let correctCount = 0;
            let totalQuestions = part === 1 ? 2 : 3;
            let startQ = part === 1 ? 1 : 3;
            
            for (let i = startQ; i < startQ + totalQuestions; i++) {
                if (finalQuizAnswers[i]) correctCount++;
            }
            
            userScore += correctCount * 10;
            updateScore();
            
            const feedback = document.getElementById(`finalFeedback${part}`);
            feedback.innerHTML = `
                <strong>Results:</strong> ${correctCount}/${totalQuestions} correct<br>
                ${correctCount === totalQuestions ? '🎉 Perfect!' : correctCount >= totalQuestions/2 ? '👍 Good job!' : '📚 Keep studying!'}
            `;
            feedback.classList.add('show', correctCount >= totalQuestions/2 ? 'correct' : 'incorrect');
        }

        function updateScore() {
            document.getElementById('currentScore').textContent = userScore;
        }

        // Drag and drop functionality
        let draggedElement = null;

        function initializeDragDrop() {
            const draggables = document.querySelectorAll('.draggable-item');
            const dropZone = document.getElementById('dropZone');
            
            draggables.forEach(item => {
                item.addEventListener('dragstart', (e) => {
                    draggedElement = e.target;
                    e.target.classList.add('dragging');
                });
                
                item.addEventListener('dragend', (e) => {
                    e.target.classList.remove('dragging');
                });
            });
            
            if (dropZone) {
                dropZone.addEventListener('dragover', (e) => {
                    e.preventDefault();
                });
                
                dropZone.addEventListener('drop', (e) => {
                    e.preventDefault();
                    if (draggedElement) {
                        dropZone.appendChild(draggedElement);
                        draggedElement = null;
                    }
                });
            }
        }

        function checkHierarchy() {
            const dropZone = document.getElementById('dropZone');
            const items = Array.from(dropZone.querySelectorAll('.draggable-item'));
            const correctOrder = ['Artificial Intelligence', 'Machine Learning', 'Deep Learning', 'Generative AI'];
            
            const userOrder = items.map(item => item.textContent);
            const isCorrect = JSON.stringify(userOrder) === JSON.stringify(correctOrder);
            
            if (isCorrect) {
                userScore += 15;
                updateScore();
                alert('🎉 Correct! You understand the AI hierarchy!');
            } else {
                alert('❌ Not quite right. Remember: AI is the broadest category, then ML, then DL, then Gen AI.');
            }
        }

        function saveReflection() {
            alert('💭 Reflection saved! Great job thinking about your AI interactions.');
            userScore += 5;
            updateScore();
        }

        function completeCourse() {
            const totalScore = document.getElementById('totalScore');
            const scoreMessage = document.getElementById('scoreMessage');
            
            totalScore.textContent = userScore;
            
            if (userScore >= 80) {
                scoreMessage.textContent = '🏆 Excellent! You\'re now AI literate!';
            } else if (userScore >= 60) {
                scoreMessage.textContent = '👍 Good job! You have a solid understanding of AI.';
            } else {
                scoreMessage.textContent = '📚 Keep learning! Review the concepts and try again.';
            }
            
            alert('🎓 Congratulations on completing the AI Literacy course!');
        }

        // Keyboard navigation
        document.addEventListener('keydown', function(e) {
            if (e.key === 'ArrowRight' || e.key === ' ') {
                nextSlide();
            } else if (e.key === 'ArrowLeft') {
                previousSlide();
            }
        });

        // Interactive elements
        document.querySelectorAll('.interactive-element').forEach(element => {
            element.addEventListener('click', function() {
                this.style.transform = 'scale(1.1) rotate(5deg)';
                setTimeout(() => {
                    this.style.transform = '';
                }, 300);
            });
        });

        // Initialize everything
        document.addEventListener('DOMContentLoaded', function() {
            console.log('Initializing AI Literacy Presentation...');
            initializeProgressTracker();
            initializeDragDrop();
            showSlide(0);
            console.log('Presentation initialized successfully!');
        });

        // New interactive functions
        function warmupAnswer(element, answer) {
            const options = element.parentNode.querySelectorAll('.answer-btn');
            options.forEach(opt => opt.classList.remove('correct'));
            element.classList.add('correct');
            
            let message = '';
            switch(answer) {
                case '0-2': message = 'You might be surprised how many AI tools you actually use!'; break;
                case '3-5': message = 'That\'s about average for most people today.'; break;
                case '6+': message = 'You\'re an AI power user! 🚀'; break;
                case 'unsure': message = 'Don\'t worry, we\'ll help you identify them!'; break;
            }
            
            setTimeout(() => alert(`💡 ${message}`), 300);
            userScore += 2;
            updateScore();
        }

        function hierarchyQuiz(element, isCorrect) {
            const options = element.parentNode.querySelectorAll('.answer-btn');
            options.forEach(opt => opt.classList.remove('correct', 'incorrect'));
            
            if (isCorrect) {
                element.classList.add('correct');
                setTimeout(() => alert('🎉 Correct! Voice recognition requires neural networks to process audio patterns.'), 300);
                userScore += 5;
            } else {
                element.classList.add('incorrect');
                setTimeout(() => alert('❌ Not quite. Voice recognition needs Deep Learning to understand speech patterns.'), 300);
            }
            updateScore();
        }

        function analyzeCreativity() {
            const input = document.getElementById('creativityInput').value;
            const result = document.getElementById('creativityResult');
            
            if (input.trim()) {
                result.innerHTML = `
                    <div style="background: rgba(78, 205, 196, 0.2); padding: 15px; border-radius: 10px; margin-top: 10px;">
                        <strong>Analysis:</strong> "${input}" shows <strong>Creative Intelligence</strong>! 
                        Current AI can assist but can't replicate your unique human creativity.
                    </div>
                `;
                result.style.display = 'block';
                userScore += 3;
                updateScore();
            } else {
                alert('Please enter something creative you did today!');
            }
        }

        function intelligenceQuiz(element, isCorrect) {
            const options = element.parentNode.querySelectorAll('.answer-btn');
            options.forEach(opt => opt.classList.remove('correct', 'incorrect'));
            
            if (isCorrect) {
                element.classList.add('correct');
                setTimeout(() => alert('🎯 Correct! Physical/kinetic intelligence requires real-world interaction, which is still challenging for AI.'), 300);
                userScore += 5;
            } else {
                element.classList.add('incorrect');
                setTimeout(() => alert('❌ Actually, kinetic (physical) intelligence is hardest - robots still struggle with basic physical tasks.'), 300);
            }
            updateScore();
        }

        function toggleLearning(element) {
            element.classList.toggle('active');
            const result = document.getElementById('learningResult');
            
            if (element.classList.contains('active')) {
                result.innerHTML = `
                    <strong>AI Learning:</strong><br>
                    ⚡ Super fast processing<br>
                    📊 Handles massive datasets<br>
                    ❌ Can't adapt to new situations easily
                `;
            } else {
                result.innerHTML = `
                    <strong>Human Learning:</strong><br>
                    🧠 Flexible and adaptable<br>
                    💡 Learns from few examples<br>
                    🤝 Understands context and emotions
                `;
            }
        }

        function checkPattern() {
            const input = document.getElementById('patternInput').value;
            const result = document.getElementById('patternResult');
            
            if (input == 48) {
                result.innerHTML = `
                    <div style="background: rgba(0, 184, 148, 0.3); padding: 15px; border-radius: 10px; color: white;">
                        🎉 Correct! Pattern: Each number doubles (×2)<br>
                        3 → 6 → 12 → 24 → 48
                    </div>
                `;
                userScore += 8;
                updateScore();
            } else {
                result.innerHTML = `
                    <div style="background: rgba(225, 112, 85, 0.3); padding: 15px; border-radius: 10px; color: white;">
                        ❌ Try again! Hint: Look at how each number relates to the previous one.
                    </div>
                `;
            }
        }

        function updateLoanPrediction(type, value) {
            document.getElementById(type + 'Weight').textContent = value;
            
            // Simple calculation for demo
            const ageWeight = parseFloat(document.getElementById('ageWeight').textContent);
            const salaryWeight = parseFloat(document.getElementById('salaryWeight').textContent);
            
            // Normalized values: age=30 (0.3), salary=190k (0.9)
            const score = Math.round((ageWeight * 30 + salaryWeight * 90) * 0.7);
            document.getElementById('loanScore').textContent = Math.min(score, 95) + '%';
        }

        function biasQuiz(element, isCorrect) {
            const options = element.parentNode.querySelectorAll('.answer-btn');
            options.forEach(opt => opt.classList.remove('correct', 'incorrect'));
            
            if (isCorrect) {
                element.classList.add('correct');
                setTimeout(() => alert('🎯 Correct! Using gender as a feature introduces unfair bias and is often illegal.'), 300);
                userScore += 5;
            } else {
                element.classList.add('incorrect');
                setTimeout(() => alert('❌ Wrong. Using gender creates bias and discrimination, which is unethical and often illegal.'), 300);
            }
            updateScore();
        }

        function activateNeuron(type) {
            const output = document.getElementById('neuronOutput');
            let result = '';
            
            switch(type) {
                case 'input1':
                    result = `
                        <strong>🔥 Input 1 Activated!</strong><br>
                        Signal: 0.8 → Hidden Layer processes → Output: 0.6<br>
                        <em>Single input creates moderate activation</em>
                    `;
                    break;
                case 'input2':
                    result = `
                        <strong>🔥 Input 2 Activated!</strong><br>
                        Signal: 0.5 → Hidden Layer processes → Output: 0.3<br>
                        <em>Weaker input creates lower activation</em>
                    `;
                    break;
                case 'both':
                    result = `
                        <strong>🔥🔥 Both Inputs Activated!</strong><br>
                        Signals: 0.8 + 0.5 → Hidden Layer combines → Output: 0.9<br>
                        <em>Combined inputs create strong activation!</em>
                    `;
                    break;
            }
            
            output.innerHTML = result;
            userScore += 2;
            updateScore();
        }

        // Capstone Project Functions
        let selectedProject = null;
        let capstoneCompleted = false;

        function selectProject(element, projectType) {
            // Remove previous selections
            document.querySelectorAll('.project-option').forEach(opt => opt.classList.remove('selected'));
            element.classList.add('selected');
            selectedProject = projectType;
            
            // Show project details
            const details = document.getElementById('projectDetails');
            details.style.display = 'block';
            
            // Update project-specific content
            updateProjectContent(projectType);
            
            userScore += 5;
            updateScore();
        }

        function updateProjectContent(projectType) {
            const examples = {
                healthcare: {
                    data: "100,000 chest X-ray images, labeled by radiologists, DICOM format",
                    architecture: "50-layer CNN with ResNet architecture for image classification",
                    metrics: "95% accuracy, <2% false positive rate, <1% false negative rate",
                    bias: "Training data might be biased toward certain demographics or hospital equipment",
                    mitigation: "Diverse training data from multiple hospitals, regular bias audits, radiologist oversight",
                    resources: "100 GPUs, 6 months, $2M budget, 10 ML engineers, 5 radiologists",
                    deployment: "Pilot with 3 hospitals, gradual rollout, continuous monitoring, FDA approval"
                },
                education: {
                    data: "Student interaction logs, assessment scores, learning preferences, video engagement",
                    architecture: "Transformer-based model with reinforcement learning for personalization",
                    metrics: "20% improvement in learning outcomes, 90% student satisfaction",
                    bias: "Algorithm might favor certain learning styles or disadvantage some student groups",
                    mitigation: "Inclusive design, diverse student testing, accessibility compliance",
                    resources: "50 GPUs, 8 months, $1.5M budget, 8 engineers, 10 educators",
                    deployment: "Beta test with 1000 students, teacher training, gradual school rollout"
                },
                environment: {
                    data: "Satellite imagery, weather data, sensor readings, historical disaster records",
                    architecture: "Multi-modal deep learning combining CNN for images and LSTM for time series",
                    metrics: "80% accuracy in disaster prediction, 24-hour advance warning",
                    bias: "Model might be biased toward well-monitored regions with more data",
                    mitigation: "Global data collection, partnership with developing nations, local validation",
                    resources: "200 GPUs, 12 months, $5M budget, 15 engineers, climate scientists",
                    deployment: "Integration with emergency services, government partnerships, public alerts"
                }
            };
            
            // Pre-fill some examples (optional - learners can override)
            const example = examples[projectType];
            if (example) {
                // You could pre-fill placeholders here if desired
            }
        }

        function submitCapstone() {
            // Check if all required fields are filled
            const inputs = document.querySelectorAll('#projectDetails input, #projectDetails textarea, #projectDetails select');
            let filledCount = 0;
            
            inputs.forEach(input => {
                if (input.value.trim() && input.value !== 'Choose AI Type...') {
                    filledCount++;
                }
            });
            
            if (filledCount < 8) {
                alert('⚠️ Please complete all sections of your capstone project!');
                return;
            }
            
            if (!selectedProject) {
                alert('⚠️ Please select a project type first!');
                return;
            }
            
            // Award capstone points
            userScore += 50;
            capstoneCompleted = true;
            updateScore();
            
            // Show success message
            alert(`🎉 Excellent work! Your ${selectedProject} AI solution is comprehensive and well-thought-out. You've demonstrated mastery of AI concepts, ethics, and practical implementation!`);
            
            // Update final score display
            document.getElementById('capstoneBonus').style.display = 'block';
            document.getElementById('totalWithCapstone').textContent = userScore;
        }

        function completeCourse() {
            const totalScore = document.getElementById('totalScore');
            const scoreMessage = document.getElementById('scoreMessage');
            
            totalScore.textContent = capstoneCompleted ? userScore - 50 : userScore;
            
            let message = '';
            let finalScore = capstoneCompleted ? userScore : userScore;
            
            if (finalScore >= 120) {
                message = '🏆 Outstanding! You\'re an AI expert with practical skills!';
            } else if (finalScore >= 100) {
                message = '🌟 Excellent! You have strong AI literacy and practical knowledge!';
            } else if (finalScore >= 80) {
                message = '👍 Great job! You understand AI fundamentals well!';
            } else if (finalScore >= 60) {
                message = '📚 Good start! Review key concepts and try the capstone project.';
            } else {
                message = '🔄 Keep learning! Retake the course to strengthen your understanding.';
            }
            
            scoreMessage.textContent = message;
            
            if (capstoneCompleted) {
                alert('🎓 Congratulations! You\'ve completed the AI Literacy course with a capstone project. You\'re ready to work with AI in the real world!');
            } else {
                alert('🎓 Course completed! Consider going back to complete the capstone project for the full experience.');
            }
        }

        function algorithmQuiz(element, isCorrect) {
            const options = element.parentNode.querySelectorAll('.answer-btn');
            options.forEach(opt => opt.classList.remove('correct', 'incorrect'));
            
            if (isCorrect) {
                element.classList.add('correct');
                setTimeout(() => alert('🎯 Correct! CNNs and ResNet are designed specifically for image analysis and pattern recognition in visual data.'), 300);
                userScore += 5;
            } else {
                element.classList.add('incorrect');
                setTimeout(() => alert('❌ Not quite. For images like X-rays, you need CNNs (Convolutional Neural Networks) or ResNet architectures.'), 300);
            }
            updateScore();
        }

        function generateCertificate() {
            const nameInput = document.getElementById('certificateName');
            const name = nameInput.value.trim();
            
            if (!name) {
                alert('⚠️ Please enter your full name to generate the certificate.');
                return;
            }
            
            // Update certificate with user's name and current date
            document.getElementById('certName').textContent = name;
            document.getElementById('certDate').textContent = `Date: ${new Date().toLocaleDateString()}`;
            
            // Show the certificate
            document.getElementById('certificateDisplay').style.display = 'block';
            
            // Add certificate bonus points
            userScore += 10;
            updateScore();
            
            // Scroll to certificate
            document.getElementById('certificate').scrollIntoView({ behavior: 'smooth' });
        }

        function downloadCertificate() {
            const certificate = document.getElementById('certificate');
            const studentName = document.getElementById('certName').textContent;
            
            html2canvas(certificate, {
                backgroundColor: '#ffffff',
                scale: 2,
                useCORS: true
            }).then(canvas => {
                // Create download link
                const link = document.createElement('a');
                link.download = `AI_Literacy_Certificate_${studentName.replace(/\s+/g, '_')}.png`;
                link.href = canvas.toDataURL('image/png');
                
                // Trigger download
                document.body.appendChild(link);
                link.click();
                document.body.removeChild(link);
                
                alert('🎉 Certificate downloaded successfully!');
            }).catch(error => {
                console.error('Error generating certificate:', error);
                alert('📸 Unable to download. Please take a screenshot of your certificate!');
            });
        }
    </script>
</body>
